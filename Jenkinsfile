pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}
// This Jenkinsfile defines a simple CI/CD pipeline with three stages: Build, Test, and Deploy.
// Each stage contains a placeholder step that echoes a message to the console.
